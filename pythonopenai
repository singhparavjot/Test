$subscriptionId = "<subscription_id>"
$resourceGroup = "<resource_group_name>"
$openAIAccountName = "<openai_resource_name>"
$startTime = "<start_time>"
$endTime = "<end_time>"

$metricParams = @{
    ResourceId = "/subscriptions/$subscriptionId/resourceGroups/$resourceGroup/providers/Microsoft.CognitiveServices/accounts/$openAIAccountName"
    MetricName = "ProcessedInferenceTokens"
    AggregationTypeName = "Total"
    StartTime = $startTime
    EndTime = $endTime
}

(Get-AzMetric @metricParams).Data
